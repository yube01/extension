{
  "name": "Health Reminder",
  "description": "Reminding extension",
  "version": "1",
  "manifest_version": 3,
  "minimum_chrome_version": "116",
  "action": {
    "default_popup": "index.html"
  },
  "host_permissions": ["https://*/*", "http://*/*"],
  "permissions": ["scripting"],
  "icons": {
    "16": "./icons/icon32.png",
    "19": "./icons/icon32.png",
    "32": "./icons/icon32.png",
    "38": "./icons/icon32.png",
    "48": "./icons/icon32.png",
    "128": "./icons/icon32.png"
  }
}
